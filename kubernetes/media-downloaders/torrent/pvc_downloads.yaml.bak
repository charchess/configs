apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-media-downloaders-torrent-downloads
  labels:
    stack: media-downloaders
    container: torrent
    volume: downloads
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-media-downloaders-torrent-downloads

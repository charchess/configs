apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-media-downloaders-amule-config
  labels:
    stack: media-downloaders
    container: amule
    volume: config
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-media-downloaders-amule-config

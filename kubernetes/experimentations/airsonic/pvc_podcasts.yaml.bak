apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-airsonic-podcasts
  labels:
    stack: experimentations
    container: airsonic
    volume: podcasts
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-airsonic-podcasts

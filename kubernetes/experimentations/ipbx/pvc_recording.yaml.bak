apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-ipbx-recording
  labels:
    stack: experimentations
    container: ipbx
    volume: recording
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-ipbx-recording

apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-pihole-dnsmasq.d
  labels:
    stack: experimentations
    container: pihole
    volume: dnsmasq.d
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-pihole-dnsmasq.d

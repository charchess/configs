apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-ejbca-persistent
  labels:
    stack: experimentations
    container: ejbca
    volume: persistent
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-ejbca-persistent

apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-kapowarr-db
  labels:
    stack: experimentations
    container: kapowarr
    volume: db
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-kapowarr-db

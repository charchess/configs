apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-authentik-custom-templates
  labels:
    stack: experimentations
    container: authentik
    volume: custom-templates
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-authentik-custom-templates

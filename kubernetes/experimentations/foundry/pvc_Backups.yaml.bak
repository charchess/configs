apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-foundry-Backups
  labels:
    stack: experimentations
    container: foundry
    volume: Backups
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-foundry-Backups

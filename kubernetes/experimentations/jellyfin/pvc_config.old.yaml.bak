apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-jellyfin-config.old
  labels:
    stack: experimentations
    container: jellyfin
    volume: config.old
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-jellyfin-config.old

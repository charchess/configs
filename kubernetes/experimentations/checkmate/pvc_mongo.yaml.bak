apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-checkmate-mongo
  labels:
    stack: experimentations
    container: checkmate
    volume: mongo
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-checkmate-mongo

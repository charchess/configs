apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-influxdb2-etc
  labels:
    stack: experimentations
    container: influxdb2
    volume: etc
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-influxdb2-etc

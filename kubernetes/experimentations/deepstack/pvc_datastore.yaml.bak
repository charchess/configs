apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-deepstack-datastore
  labels:
    stack: experimentations
    container: deepstack
    volume: datastore
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-deepstack-datastore

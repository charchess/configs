apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-graylog-mongodb_config
  labels:
    stack: experimentations
    container: graylog
    volume: mongodb_config
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-graylog-mongodb_config

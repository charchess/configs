  labels:
    stack: experimentations
    container: magicmirror
    volume: modules
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-magicmirror-modules

apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-obsidian-vaults
  labels:
    stack: experimentations
    container: obsidian
    volume: vaults
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-obsidian-vaults

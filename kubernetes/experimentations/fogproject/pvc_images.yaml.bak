apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-fogproject-images
  labels:
    stack: experimentations
    container: fogproject
    volume: images
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-fogproject-images

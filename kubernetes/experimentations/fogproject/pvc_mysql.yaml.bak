apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-fogproject-mysql
  labels:
    stack: experimentations
    container: fogproject
    volume: mysql
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-fogproject-mysql

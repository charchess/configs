apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-ollama-ollama
  labels:
    stack: experimentations
    container: ollama
    volume: ollama
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-ollama-ollama

apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-experimentations-zwave2mqtt-sessions
  labels:
    stack: experimentations
    container: zwave2mqtt
    volume: sessions
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-experimentations-zwave2mqtt-sessions

apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-logs-opensearch-data
  labels:
    stack: logs
    container: opensearch
    volume: data
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-logs-opensearch-data

metadata:
  name: pvc-infrastructure-awx_task-data
  labels:
    stack: infrastructure
    container: awx_task
    volume: data
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-infrastructure-awx_task-data

apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-ai_services-embedding-service-models_cache
  labels:
    stack: ai_services
    container: embedding-service
    volume: models_cache
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-ai_services-embedding-service-models_cache

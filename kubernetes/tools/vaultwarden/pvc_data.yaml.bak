apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-tools-vaultwarden-data
  labels:
    stack: tools
    container: vaultwarden
    volume: data
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-tools-vaultwarden-data

apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: pvc-media-management-pyload-tmp
  labels:
    stack: media-management
    container: pyload
    volume: tmp
spec:
  accessModes:
    - ReadWriteOnce
  resources:
    requests:
      storage: 1Gi
  storageClassName: nfs
  volumeName: pv-pvc-media-management-pyload-tmp
